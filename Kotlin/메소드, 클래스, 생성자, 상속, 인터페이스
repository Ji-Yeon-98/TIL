    sparrow.fly()
    pigeon.fly()
}

open class Bird(name:String) {
    var name: String = ""

    init {
        // thisëŠ” í˜„ì¬ í´ë˜ìŠ¤ì˜ ìƒíƒœë³€ìˆ˜ë¥¼ ì˜ë¯¸í•©ë‹ˆë‹¤
